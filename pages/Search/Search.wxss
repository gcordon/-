@import '../../utils/Film.wxss';
#searchMain {
    width: 100%;
}
#searchMain .s-input{
    width: 100%;
    height: 100rpx;
}
#searchMain .s-input input {
    text-indent: 50rpx;
    font-size: 32rpx;
    width: 100%;
    height: 100%;
    background-color: #eee;
    box-sizing: border-box;
    padding: 5rpx;
    border: 5px solid #fff;
    color: #000;
}
#searchMain .s-input button {
    background-color: red;
}
.hr {
    width: 100%;
    height: 80rpx;
    background-color: transparent;
    padding-bottom: 40rpx;
}